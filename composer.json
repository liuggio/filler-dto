{
    "name": "liuggio/filler-dto",
    "license": "MIT",
    "description": "Retrieve and set properties from and to a DTO",
    "authors": [
        {
            "name": "liuggio",
            "email": "liuggio@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~4",
        "symfony/http-foundation": "~2.7"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "require": {
        "php": ">=5.3.2"
    },
    "autoload": {"psr-4": {"Liuggio\\Filler\\": "src"}},
    "autoload-dev": {
        "psr-4": {
            "Liuggio\\Filler\\Test\\": "tests"
        }
    },
    "suggest": {
        "symfony/http-foundation": "Install symfony http-foundation in order to use the Response filler"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
